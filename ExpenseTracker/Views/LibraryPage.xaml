<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ExpenseTracker.Views.LibraryPage"
             Title="Library Page">
    
    
    <StackLayout Padding="20" Spacing="10">
        <Label Text="Category Entry" 
           FontAttributes="Bold"
           FontSize="16"
           Padding="5"/>
        <Button Text="Category Entry"
                Command="{Binding NavigateToCategoryPageCommand}" />

    </StackLayout>
</ContentPage>